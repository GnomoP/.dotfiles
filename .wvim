[options]
allow_bold = true
audible_bell = false
clickable_url = true
dynamic_title = true
hyperlinks = true
icon_name = terminal
mouse_autohide = true
scroll_on_output = false
scroll_on_keystroke = true

# Length of the scrollback buffer, 0 disabled the scrollback buffer
# and setting it to a negative value means "infinite scrollback"
scrollback_lines = -1

# "system", "on" or "off"
cursor_blink = on

# "block", "underline" or "ibeam"
cursor_shape = block

# Hide links that are no longer valid in url select overlay mode
filter_unmatched_urls = true

# "off", "left" or "right"
scrollbar = off

font = Menlo for Powerline 10

#fullscreen = true
#search_wrap = true
#urgent_on_bell = true

# $BROWSER is used by default if set, with xdg-open as a fallback
#browser = xdg-open

# Emit escape sequences for extra modified keys
#modify_other_keys = false

# Set size hints for the window
#size_hints = false

[colors]

# Cursor
# If both of these are unset, cursor falls back to the foreground color,
# and cursor_foreground falls back to the background color.
cursor =            #5C6370
cursor_foreground = #1E2127

# Selection
# If unset, will reverse foreground and background
highlight = #3A3F4B

# Background
# 10% background transparency (requires a compositor)
background = rgba(30, 33, 39, 0.90)

# Text
foreground =      #ABB2BF
foreground_bold = #ABB2BF

# Black
color0 =  #1E2127
color8 =  #5C6370

# Red
color1 =  #E06C75
color9 =  #E06C75

# Green
color2 =  #98C379
color10 = #98c379

# Yellow
color3 =  #D19A66
color11 = #D19A66

# Blue
color4 =  #61AFEF
color12 = #61AFEF

# Magenta
color5 =  #C678DD
color13 = #C678DD

# Cyan
color6 =  #56B6C2
color14 = #56B6C2

# White
color7 =  #ABB2BF
color15 = #FFFFFF
